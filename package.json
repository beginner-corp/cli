{
  "name": "begin-cli",
  "version": "0.0.0",
  "bin": {
    "begin": "src/index.js"
  },
  "main": "src/index.js",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "test": "npm run lint && npm run coverage && npm run test:integration",
    "test:unit": "cross-env tape 'test/unit/**/*-test.js' | tap-arc",
    "test:integration": "cross-env tape 'test/integration/**/*-test.js' | tap-arc",
    "coverage": "nyc --reporter=lcov --reporter=text npm run test:unit",
    "lint": "eslint . --fix",
    "build": "scripts/binary-config && npx pkg scripts && rm -f commit",
    "deploy": "BUILD_ALL=true npm run build && scripts/deploy"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@architect/inventory": "2.2.1",
    "@architect/parser": "5.0.2",
    "@architect/sandbox": "4.4.1",
    "chalk": "4.1.2",
    "lambda-runtimes": "1.1.1",
    "minimist": "1.2.5",
    "strip-ansi": "6.0.1"
  },
  "devDependencies": {
    "@architect/eslint-config": "^2.0.1",
    "adm-zip": "~0.5.9",
    "aws-sdk": "~2.1036.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.3.0",
    "nyc": "^15.1.0",
    "pkg": "^5.4.1",
    "proxyquire": "~2.1.3",
    "rimraf": "~3.0.2",
    "tap-arc": "~0.1.1",
    "tap-spec": "^5.0.0",
    "tape": "^5.3.2"
  },
  "eslintConfig": {
    "extends": "@architect/eslint-config",
    "rules": {
      "global-require": "off"
    }
  }
}
